  "@type" : [ "http://www.w3.org/2002/07/owl#NamedIndividual", "http://cpsagila.cs.uni-kl.de/Roadmap/OntologyForInnovation#DevelopmentStage" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "Develop a prototype allowing for some ‘user preference’ analysis which can be fed back to the designers."
  } ]
}, {
  "@id" : "http://cpsagila.cs.uni-kl.de/Roadmap/OntologyForInnovation/d4-demonstrating-the-benefits",
  "@type" : [ "http://www.w3.org/2002/07/owl#NamedIndividual", "http://cpsagila.cs.uni-kl.de/Roadmap/OntologyForInnovation#DevelopmentStage" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "Test and demonstrate both the functional and non-functional qualities of the solution, emphasising the beneftis to the user."
  } ]
}, {
